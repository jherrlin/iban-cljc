;; shadow-cljs configuration
{:source-paths
 ["src" "dev"]

 :builds
 {:app {:target     :browser
        :output-dir "dev/public/js"
        :asset-path "/js"

        :modules
        {:main ;; <- becomes public/js/main.js
         {:entries [se.jherrlin.iban.view]}}

        ;; start a development http server on http://localhost:8020
        :devtools
        {:http-root "dev/public"
         :http-port 8020}}}}
